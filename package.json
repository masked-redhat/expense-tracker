{
    "name": "expense-tracker",
    "version": "1.0.0",
    "description": "express backend setup for projects with no sql and sql databases",
    "keywords": [
        "expense-tracker"
    ],
    "homepage": "https://github.com/masked-redhat/expense-tracker#readme",
    "bugs": {
        "url": "https://github.com/masked-redhat/expense-tracker/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/masked-redhat/expense-tracker.git"
    },
    "license": "ISC",
    "author": "masked-redhat",
    "type": "module",
    "main": "app.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "npm i",
        "dev": "nodemon --signal SIGTERM app.js",
        "start": "node app.js",
        "redis": "node ./scripts/start_redis.js",
        "rdev": "npm run redis && npm run dev",
        "rstart": "npm run redis && npm run start"
    },
    "dependencies": {
        "bcryptjs": "latest",
        "cookie-parser": "latest",
        "express": "latest",
        "express-setup": "file:",
        "jsonwebtoken": "latest",
        "mongoose": "latest",
        "multer": "latest",
        "nodemailer": "latest",
        "redis": "latest",
        "socket.io": "latest",
        "uuid": "latest"
    },
    "devDependencies": {
        "dotenv": "latest",
        "helmet": "latest",
        "nodemon": "latest"
    }
}
